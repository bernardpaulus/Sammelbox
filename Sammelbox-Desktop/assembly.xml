<?xml version="1.0" encoding="UTF-8"?>
<assembly>
   <id>dist</id>
   <formats>
      <format>jar</format>
   </formats>
   <includeBaseDirectory>false</includeBaseDirectory>
   <fileSets>
      <fileSet>
         <directory>target</directory>
         <outputDirectory />
         <includes>
            <include>lib/*.jar</include>
         </includes>
      </fileSet>
   </fileSets>
   <dependencySets>
      <dependencySet>
         <outputDirectory />
         <unpack>true</unpack>
         <scope>runtime</scope>
      </dependencySet>
   </dependencySets>
</assembly>

